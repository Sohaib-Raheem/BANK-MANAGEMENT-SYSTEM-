CREATE TABLE Cards (
    CardID INT AUTO_INCREMENT PRIMARY KEY,
    AccountID INT,
    CardType VARCHAR(50),
    ExpiryDate DATE,
    FOREIGN KEY (AccountID) REFERENCES Accounts(AccountID)
);

